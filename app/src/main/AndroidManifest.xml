<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hbm.heathbaron_morgan.reflexmath">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.hbm.heathbaron_morgan.reflexmath.MainMenu"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.hbm.heathbaron_morgan.reflexmath.StatsPage"
            android:label="@string/title_activity_main2"
            android:launchMode="singleTask"
            android:theme="@style/AppTheme" />
        <activity
            android:name="com.hbm.heathbaron_morgan.reflexmath.SettingsPage"
            android:launchMode="singleTask" />
        <activity
            android:name="com.hbm.heathbaron_morgan.reflexmath.GamePage"
            android:launchMode="singleTask" />
        <activity android:name="com.hbm.heathbaron_morgan.reflexmath.DifficultyPage" />
        <activity android:name="com.hbm.heathbaron_morgan.reflexmath.GameOverPage" />
        <activity android:name="com.hbm.heathbaron_morgan.reflexmath.GameType"></activity>
    </application>

</manifest>